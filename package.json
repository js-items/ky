{
  "name": "@js-items/ky",
  "version": "0.0.0-development",
  "description": "Concrete implementation of js-items for ky",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "duplication": "jscpd ./src",
    "build": "npm run clean && tsc",
